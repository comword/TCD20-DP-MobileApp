    "@reduxjs/toolkit": "^1.5.1",
    "expo": "~41.0.1",
    "expo-crypto": "~9.1.0",
    "expo-splash-screen": "~0.10.2",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.5.4",
    "google-protobuf": "^3.17.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-is": "16.13.1",
    "react-native": "~0.63.4",
    "react-native-appearance": "^0.3.4",
