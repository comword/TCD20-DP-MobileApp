{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "es6",
      "es2016.array.include",
      "es2017.object"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-native",
    "strict": true,
    "baseUrl": "src"
  },
  "extends": "expo/tsconfig.base"
}
